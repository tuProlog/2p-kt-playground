{
  "name": "npm-try",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "build": "app/**/*.*",
    "test": {
      "patterns": [
        "app/**",
        "test/**"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "start": "npm-watch",
    "watch": "npm-watch",
    "build": "browserify app/control.js -o dist/bundle.js && npm run copy",
    "copy": "ncp app/index.html dist/index.html && ncp app/style.css dist/style.css",
    "test": "mocha"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tuprolog/2p-core": "latest",
    "@tuprolog/2p-parser-theory": "latest",
    "@tuprolog/2p-solve-classic": "latest",
    "@tuprolog/2p-theory": "latest"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "ncp": "^2.0.0",
    "npm-watch": "^0.6.0",
    "tinyify": "^2.5.2"
  }
}
